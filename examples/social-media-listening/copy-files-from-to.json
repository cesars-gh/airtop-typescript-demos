{
  "copyFilesSettings": {
    "whenFileExists": "notify-about-available-change"
  },
  "copyFiles": [
    {
      "from": "node_modules/@local/ui/assets/public/**/*",
      "to": "public"
    },
    {
      "from": "node_modules/@local/ui/assets/app/**/*",
      "to": "src/app"
    },
    {
      "from": "node_modules/@internal/home-config/.common-env",
      "to": ".env"
    }
  ]
}